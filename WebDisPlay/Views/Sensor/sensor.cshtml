<body>
    <div id="body" style="width:100%; height:100%;">
        <div id="left-select" style="width:20%; height: 100%; float:left">
            <div id="filter1" style="padding-top:15%;padding-left:8%">
                <p1>TBIM</p1>
                <select data-am-selected="{btnWidth: '40%', btnSize: 'sm', btnStyle: 'secondary'}">
                    <option value="a">All</option>
                    <option value="b">TBIM-1</option>
                    <option value="o">TBIM-2</option>
                    <option value="m">TBIM-3</option>
                </select>
            </div>
            <div id="filter2" style="padding-top:15%;padding-left:8%">
                <p1>TYPE</p1>
                <select data-am-selected="{btnWidth: '40%', btnSize: 'sm', btnStyle: 'secondary'}">
                    <option value="a">Speed</option>
                    <option value="b">Temperature</option>
                    <option value="o">Fuel quantity</option>
                    <option value="m">Acceleration</option>
                </select>
            </div>
            <div id="sensor8" style="width:20%; height: 30%; padding-left:8%;">
                <div id="switch-text" style="height:50%;text-align:center"><p1>Switch</p1></div>
                <div id="swith-graph" style="height:15%;text-align:center"><input type="checkbox" data-am-switch data-off-color="warning" /></div>
            </div>
        </div>

        <div id="tbims" style="width: 80%; float: left">
            <input type="checkbox" class="js-switch" checked />
            <script type="text/javascript" charset="utf-8">
                var elem = document.querySelector('.js-switch', {size : 'large'});
                var init = new Switchery(elem);

            </script>
        </div>
        
     </div>

